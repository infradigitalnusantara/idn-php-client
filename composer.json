{
  "name": "infradigitalnusantara/idn-php-client",
  "type": "library",
  "description": "Infra Digital Nusantara APIs Client Adapter",
  "homepage": "https://www.infradigital.io",
  "authors": [
    {
      "name": "Infra Digital Nusantara",
      "homepage": "https://github.com/infra-digital/php-client-schools"
    }
  ],
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "InfraDigital\\ApiClient\\": "src/"
    }
  },
  "require": {
    "php": ">=5.6",
    "psr/http-message": "^1.0",
    "psr/cache": "^1.0",
    "php-http/httplug": "^1.1",
    "php-http/discovery": "^1.0",
    "php-http/client-implementation": "^1.0",
    "php-http/client-common": "^1.3",
    "php-http/cache-plugin": "^1.2",
    "php-http/logger-plugin": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0 || ^5.5",
    "php-http/guzzle6-adapter": "^1.0",
    "guzzlehttp/psr7": "^1.2",
    "fzaninotto/faker": "^1.8"
  }
}
